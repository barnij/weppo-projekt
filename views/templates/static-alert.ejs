<% if(locals.alert){ %>
    <div class="container">
        <div class="row">
            <div class="col">
                <div class="alert alert-<%= locals.alert.type %>  alert-dismissible fade show" role="alert">
                    <%= locals.alert.message %>
                        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                </div>
            </div>
        </div>
    </div>
<% } %>